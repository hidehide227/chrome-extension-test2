{
    "name": "Chrome Extension Test2",
    "description": "Chrome Extension Test2",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "unlimitedStorage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon1.png"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+M",
                "mac": "MacCtrl+M"
            },
            "description": "Run extension."
        }
    }
}